# CREATE A FLASK APP

from flask import  Flask

app = Flask(__name__)  # __name__ as an arg to Flask, you are providing the name of the current module to the Flask

@app.route("/")
def index():
    return rf"""
My FIRST FLASK APP IS GOOD
"""
if __name__ == "__main__":  # ensures that the following code block is only executed if the module is being run directly
    app.run(debug=True)
